##1

k = input("give me your list of number(use space between nums): ")
k = k.split()
m=[]
for i in k:
    i = int(i)
    m.append(i)
def myfunction(m):
    while len(m)>1:
         if m[0] > m[1]:
            m.pop(1)
         elif m[0] < m[1]:
            m.pop(0)
    print(m[0])
myfunction(m)

